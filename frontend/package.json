{
  "dependencies": {
    "@tailwindcss/cli": "^4.1.3",
    "tailwindcss": "^4.1.3"
  },
  "scripts": {
    "build": "npx @tailwindcss/cli -i ./src/CSS/input.css -o ./src/CSS/output.css",
    "watch": "npx @tailwindcss/cli -i ./src/CSS/input.css -o ./src/CSS/output.css --watch",
    "dist": "tailwindcss -i ./src/CSS/input.css -o ./dist/output.css --minify"
  }
}
